{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/audio.js",
            "destination": "/frontend/audio.js"
        },
        {
            "source": "/",
            "destination": "/frontend/frontend.html"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}